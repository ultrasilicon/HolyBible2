QPushButton{
    background-color: #e7e7e7;
    border-style: solid;
    border-width: 3px;
    border-radius: 0px;
    border-color: #b69465;
    font: 11px bold;
    color: #9f6841;
    min-width: 7em;
    height: 10;
    padding: 6px;
}

/*QMainWindow{
